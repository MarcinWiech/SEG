<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="123.0" prefWidth="331.0"
            xmlns="http://javafx.com/javafx/10.0.1"
            fx:controller="seg.java.controllers.config.AirportCreationController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" prefHeight="21.0" prefWidth="200.0" text="CONFIGURE A NEW AIRPORT">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="48.0" text="Name:"/>
        <TextField fx:id="airportNameTextbox" layoutX="74.0" layoutY="44.0" prefHeight="25.0" prefWidth="184.0"/>
        <Button fx:id="addAirportButton" layoutX="274.0" layoutY="80.0" mnemonicParsing="false" onAction="#addAirport"
                text="Add"/>
    </children>
</AnchorPane>
