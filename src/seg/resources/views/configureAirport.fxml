<AnchorPane prefHeight="159.0" prefWidth="334.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seg.java.controllers.ConfigureAirportController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="21.0" prefWidth="200.0" text="CONFIGURE A NEW AIRPORT">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="86.0" layoutY="47.0" text="Name:" />
      <Label layoutX="14.0" layoutY="83.0" text="Number of runways:" />
      <TextField fx:id="airportNameTextbox" layoutX="134.0" layoutY="43.0" prefHeight="25.0" prefWidth="184.0" />
      <ChoiceBox fx:id="runwayDroplist" layoutX="134.0" layoutY="79.0" prefHeight="25.0" prefWidth="74.0" />
      <Button fx:id="addAirportButton" layoutX="280.0" layoutY="115.0" mnemonicParsing="false" onAction="#addAirport" text="Add" />
      <Button fx:id="backToAirportSelection" layoutX="14.0" layoutY="115.0" mnemonicParsing="false" onAction="#backToAirportSelection" text="Back" />
      <Button fx:id="importAirportButton" layoutX="75.0" layoutY="115.0" mnemonicParsing="false" onAction="#importAirportsFromXML" prefHeight="25.0" prefWidth="184.0" text="Import an Airport" />
   </children>
