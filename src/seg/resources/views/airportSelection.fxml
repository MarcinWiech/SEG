<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="139.0" prefWidth="333.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="seg.java.controllers.AirportSelectionController">
    <children>
        <Label layoutX="30.0" layoutY="28.0" prefHeight="30.0" prefWidth="142.0" text="Select an airportOld: ">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <ChoiceBox fx:id="airportDroplist" layoutX="17.0" layoutY="79.0" prefHeight="25.0" prefWidth="169.0"/>
        <Button fx:id="selectButton" layoutX="204.0" layoutY="79.0" mnemonicParsing="false" onAction="#openDashboard"
                prefHeight="50.0" prefWidth="101.0" text="Select"/>
        <Label fx:id="addAirportLabel" layoutX="40.0" layoutY="116.0" onMouseClicked="#openAirportConfigurationScreen"
               onMouseEntered="#underlineText" onMouseExited="#stopUnderlineText"
               onTouchPressed="#openAirportConfigurationScreen" prefHeight="19.0" prefWidth="108.0"
               text="Old (delete me)" textAlignment="CENTER">
            <font>
                <Font size="13.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
