<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="318.0" prefWidth="362.0"
            xmlns="http://javafx.com/javafx/8.0.121" fx:controller="seg.java.controllers.ConfigureRunwayController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="CONFIGURE RUNWAY">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="48.0" text="Runway designator: "/>
        <Label layoutX="101.0" layoutY="84.0" text="TORA:"/>
        <Label layoutX="100.0" layoutY="119.0" text="TODA:"/>
        <Label layoutX="101.0" layoutY="152.0" text="ASDA:"/>
        <Label layoutX="109.0" layoutY="185.0" text="LDA:"/>
        <TextField fx:id="runwayDesignatorTextbox" layoutX="148.0" layoutY="44.0" prefHeight="25.0" prefWidth="107.0"/>
        <TextField fx:id="toraTextbox" layoutX="148.0" layoutY="79.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="todaTextbox" layoutX="148.0" layoutY="114.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="asdaTextbox" layoutX="148.0" layoutY="147.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="ldaTextbox" layoutX="148.0" layoutY="180.0" prefHeight="25.0" prefWidth="65.0"/>
        <Label layoutX="75.0" layoutY="217.0" text="Threshold:"/>
        <Button fx:id="backButton" layoutX="14.0" layoutY="253.0" mnemonicParsing="false"
                onAction="#backToConfigureAirport" text="Back"/>
        <Button fx:id="addRunwayButton" layoutX="140.0" layoutY="253.0" mnemonicParsing="false" onAction="#addRunway"
                prefHeight="31.0" prefWidth="107.0" text="Add Another"/>
        <TextField fx:id="thresholdTextbox" layoutX="149.0" layoutY="213.0" prefHeight="25.0" prefWidth="65.0"/>
        <Button fx:id="finishedButton" layoutX="256.0" layoutY="253.0" mnemonicParsing="false" onAction="#onFinish"
                prefHeight="31.0" prefWidth="89.0" text="Finished"/>
    </children>
</AnchorPane>
