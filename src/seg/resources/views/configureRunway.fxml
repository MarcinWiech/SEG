<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="293.0" prefWidth="375.0"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="seg.java.controllers.ConfigureRunwayController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="CONFIGURE RUNWAY">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        <Label layoutX="30.0" layoutY="48.0" text="Runway designator: "/>
        <Label layoutX="101.0" layoutY="83.0" text="TORA:"/>
        <Label layoutX="100.0" layoutY="118.0" text="TODA:"/>
        <Label layoutX="101.0" layoutY="151.0" text="ASDA:"/>
        <Label layoutX="109.0" layoutY="184.0" text="LDA:"/>
        <TextField fx:id="runwayDesignatorTextbox" layoutX="148.0" layoutY="44.0" prefHeight="25.0" prefWidth="107.0"/>
        <TextField fx:id="toraTextbox" layoutX="148.0" layoutY="79.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="todaTextbox" layoutX="148.0" layoutY="114.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="asdaTextbox" layoutX="148.0" layoutY="147.0" prefHeight="25.0" prefWidth="65.0"/>
        <TextField fx:id="ldaTextbox" layoutX="148.0" layoutY="180.0" prefHeight="25.0" prefWidth="65.0"/>
        <Label layoutX="81.0" layoutY="217.0" text="Threshold:"/>
        <Button fx:id="backButton" layoutX="14.0" layoutY="253.0" mnemonicParsing="false"
                onAction="#backToConfigureAirport" text="Back"/>
        <Button fx:id="addRunwayButton" layoutX="296.0" layoutY="253.0" mnemonicParsing="false" onAction="#addRunway"
                text="Add Next"/>
        <TextField fx:id="thresholdTextbox" layoutX="148.0" layoutY="213.0" prefHeight="25.0" prefWidth="65.0"/>
    </children>
</AnchorPane>
