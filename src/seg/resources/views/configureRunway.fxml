        <Label layoutX="101.0" layoutY="111.0" text="TORA:" />
        <Label layoutX="100.0" layoutY="146.0" text="TODA:" />
        <Label layoutX="101.0" layoutY="179.0" text="ASDA:" />
        <Label layoutX="109.0" layoutY="212.0" text="LDA:" />
        <TextField fx:id="runwayDesignatorTextbox" layoutX="148.0" layoutY="44.0" prefHeight="25.0" prefWidth="107.0" />
        <TextField fx:id="toraTextbox" layoutX="148.0" layoutY="107.0" prefHeight="25.0" prefWidth="65.0" />
        <TextField fx:id="todaTextbox" layoutX="148.0" layoutY="142.0" prefHeight="25.0" prefWidth="65.0" />
