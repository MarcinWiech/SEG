      <Button fx:id="finishedButton" layoutX="260.0" layoutY="279.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="66.0" text="Finished" />
      <Label layoutX="32.0" layoutY="79.0" text="Reciprocal runway:" />
      <TextField fx:id="reciprocalRunwayTextbox" layoutX="148.0" layoutY="75.0" prefHeight="25.0" prefWidth="107.0" />
    </children>
</AnchorPane>
