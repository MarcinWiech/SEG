<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="162.0" prefWidth="244.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seg.java.controllers.LoginViewController">
    <children>
        <Label alignment="CENTER" layoutX="20.0" layoutY="14.0" prefHeight="30.0" prefWidth="201.0" text="Runway Redeclaration Tool">
            <font>
                <Font name="System Bold" size="15.3" />
            </font>
        </Label>
        <Button fx:id="loginButton" layoutX="20.0" layoutY="114.0" mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="201.0" text="Login" />
        <TextField fx:id="loginTextField" layoutX="20.0" layoutY="49.0" prefHeight="26.0" prefWidth="201.0" promptText="Username" />
        <PasswordField fx:id="passwordTextField" layoutX="20.0" layoutY="79.0" prefHeight="26.0" prefWidth="201.0" promptText="Password" />
    </children>
</AnchorPane>
