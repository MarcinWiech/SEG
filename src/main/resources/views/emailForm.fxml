   <children>
      <TextField fx:id="emailTextbox" layoutX="14.0" layoutY="35.0" prefHeight="25.0" prefWidth="205.0" />
      <Label layoutX="14.0" layoutY="10.0" text="Enter the recipients email below:" />
      <Button fx:id="sendButton" layoutX="176.0" layoutY="67.0" mnemonicParsing="false" onAction="#sendEmail" prefHeight="0.0" prefWidth="43.0" text="Send">
