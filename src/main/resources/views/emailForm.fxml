<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="100.0" prefWidth="235.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seg.java.controllers.EmailController">
   <children>
      <TextField fx:id="emailTextbox" layoutX="14.0" layoutY="35.0" prefHeight="25.0" prefWidth="205.0" promptText="Email Address" />
      <Label alignment="CENTER" layoutX="5.0" layoutY="10.0" prefHeight="15.0" prefWidth="225.0" text="Enter the recipients email below:" />
      <Button fx:id="sendButton" layoutX="152.0" layoutY="67.0" mnemonicParsing="false" onAction="#sendEmail" prefHeight="22.0" prefWidth="67.0" text="Send">
         <font>
            <Font size="11.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
